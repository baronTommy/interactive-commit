{
  "name": "interactive-commit",
  "description": "interactive snippet tool",
  "version": "1.5.1",
  "author": "akira-toriyama",
  "main": "lib/opt/index.js",
  "bin": {
    "interactive-commit": "./bin/run"
  },
  "dependencies": {
    "@octokit/rest": "18.12.0",
    "cac": "6.7.11",
    "chalk": "4.1.2",
    "cosmiconfig": "7.0.1",
    "gitmojis": "3.5.0",
    "inquirer": "8.2.0",
    "inquirer-search-list": "1.2.6",
    "table": "6.7.2",
    "tsc-alias": "1.3.10"
  },
  "devDependencies": {
    "@semantic-release/changelog": "6.0.0",
    "@semantic-release/git": "10.0.0",
    "@semantic-release/npm": "8.0.2",
    "@types/chalk": "2.2.0",
    "@types/inquirer": "8.1.3",
    "@types/jest": "27.0.2",
    "@types/node": "16.11.6",
    "@typescript-eslint/eslint-plugin": "5.1.0",
    "@typescript-eslint/parser": "5.1.0",
    "conventional-changelog-conventionalcommits": "4.6.1",
    "eslint": "8.1.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-jest": "25.2.2",
    "interactive-commit": "1.1.3",
    "jest": "27.3.1",
    "jscpd": "3.3.26",
    "lint-staged": "11.2.4",
    "npm-run-all": "4.1.5",
    "prettier": "2.4.1",
    "semantic-release": "18.0.0",
    "ts-jest": "27.0.7",
    "ts-node": "10.4.0",
    "tsconfig-paths": "3.11.0",
    "tslib": "2.3.1",
    "typescript": "4.4.4"
  },
  "files": [
    "/bin",
    "/lib",
    "README.md"
  ],
  "homepage": "https://github.com/baronTommy/interactive-commit",
  "keywords": [
    "commit",
    "conventional-commits",
    "conventional",
    "conventional changelog",
    "semantic-version",
    "semantic-release",
    "gitmoji"
  ],
  "license": "MIT",
  "repository": "baronTommy/interactive-commit",
  "scripts": {
    "quality": "run-p quality:*",
    "quality:lint-fmt": "run-s lint fmt",
    "quality:unitTest": "jest --testPathIgnorePatterns .gitignore",
    "quality:jscpd": "jscpd ./app",
    "quality:type": "tsc --project ./tsconfig.json --noEmit",
    "lint": "eslint --fix --ignore-path .gitignore .",
    "fmt": "prettier --write --ignore-path .gitignore .",
    "prepack": "npm run quality && rm -rf lib && tsc --project tsconfig.build.json && tsc-alias -p tsconfig.build.json",
    "prepare": "git config --local core.hooksPath .githooks"
  }
}
